application.title=Toleco
application.vendor=Pooter
build.classes.dir=${build.dir}/classes
build.classes.excludes=**/*.java,**/*.form
# This directory is removed when the project is cleaned:
build.dir=build
build.generated.dir=${build.dir}/generated
build.generated.sources.dir=${build.dir}/generated-sources
# Only compile against the classpath explicitly listed here:
build.sysclasspath=ignore
build.test.classes.dir=${build.dir}/test/classes
build.test.results.dir=${build.dir}/test/results
# Uncomment to specify the preferred debugger connection transport:
#debug.transport=dt_socket
debug.classpath=\
    ${run.classpath}
debug.test.classpath=\
    ${run.test.classpath}
# This directory is removed when the project is cleaned:
dist.dir=dist
dist.jar=${dist.dir}/Toleco.jar
dist.javadoc.dir=${dist.dir}/javadoc
endorsed.classpath=
excludes=
file.reference.308checks.jar=lib/308checks.jar
file.reference.abbot.jar=../../../Download/abbot.jar
file.reference.abbot.jar-1=lib/abbot.jar
file.reference.AbsoluteLayout.jar=lib/AbsoluteLayout.jar
file.reference.checkstyle-all-4.4.jar=lib/checkstyle-all-4.4.jar
file.reference.costello.jar=../../../Download/costello.jar
file.reference.costello.jar-1=lib/costello.jar
file.reference.gnu-regexp-1.1.0.jar=lib/gnu-regexp-1.1.0.jar
file.reference.jdom-1.0.jar=lib/jdom-1.0.jar
file.reference.jgraph.jar=lib/jgraph.jar
file.reference.junit-4.8.1.jar=lib/junit-4.8.1.jar
file.reference.LOC.jar=lib/LOC.jar
includes=**
jar.compress=false
javac.classpath=\
    ${file.reference.junit-4.8.1.jar}:\
    ${file.reference.LOC.jar}:\
    ${file.reference.checkstyle-all-4.4.jar}:\
    ${file.reference.308checks.jar}:\
    ${file.reference.AbsoluteLayout.jar}:\
    ${file.reference.abbot.jar-1}:\
    ${file.reference.costello.jar-1}:\
    ${file.reference.gnu-regexp-1.1.0.jar}:\
    ${file.reference.jdom-1.0.jar}:\
    ${file.reference.jgraph.jar}
# Space-separated list of extra javac options
javac.compilerargs=-Xlint:unchecked
javac.deprecation=false
javac.source=1.5
javac.target=1.5
javac.test.classpath=\
    ${javac.classpath}:\
    ${build.classes.dir}
javadoc.additionalparam=-tag pre:a:"Precondition" -tag post:a:"Postcondition: "
javadoc.author=true
javadoc.encoding=${source.encoding}
javadoc.noindex=false
javadoc.nonavbar=false
javadoc.notree=false
javadoc.private=true
javadoc.splitindex=true
javadoc.use=true
javadoc.version=true
javadoc.windowtitle=Toleco
javadoc.overview=overview.html
jaxbwiz.endorsed.dirs="${netbeans.home}/../ide12/modules/ext/jaxb/api"
main.class=toleco.TolecoDriver
manifest.file=manifest.mf
meta.inf.dir=${src.dir}/META-INF
platform.active=default_platform
run.classpath=\
    ${javac.classpath}:\
    ${build.classes.dir}
# Space-separated list of JVM arguments used when running the project
# (you may also define separate properties like run-sys-prop.name=value instead of -Dname=value
# or test-sys-prop.name=value to set system properties for unit tests):
run.jvmargs=
run.test.classpath=\
    ${javac.test.classpath}:\
    ${build.test.classes.dir}
source.encoding=UTF-8
src.dir=src
test.src.dir=test
